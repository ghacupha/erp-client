    "cypress": "10.7.0",
    "cypress-audit": "1.1.0",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-webpack-plugin": "3.2.0",
    "generator-jhipster": "7.9.3",
    "jest": "28.1.3",
